    "fastify": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "nodemon": "^2.0.18",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
